((defn all-caps? [prompt]
  (not (nil? (re-matches #"^[A-Z\,\.\s\?]+$" prompt))))
)
(re-matches #"^[A-Z\,\.\s\?]+$" "W O!")
(re-matches #"^[A-Z\,\.\s\?\!]+$" "W O!")
(re-matches #"^[A-Z\w\s)]|[_]+$" "W O!")
(re-matches #"^[A-Z\w\s\)\]\|\[\_\]]+$" "W O!")
(re-matches #"[\w+]" "WO")
(re-matches #"[\w+]" "WO!")
(re-matches #"[\w+]" "!")
(re-matches #"[\w+]" "^")
(re-matches #"[\w]+" "^")
(re-matches #"[^\w]" "^")
(re-matches #"[^\w]" "!")
